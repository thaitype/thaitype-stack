    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "chokidar": "^4.0.3",
    "esbuild": "^0.25.4",
    "execa": "^8.0.0",
    "npm-run-all2": "^8.0.3",
    "tsx": "^4.19.4",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@azure/functions": "^4.7.2",
    "@azure/static-web-apps-cli": "^2.0.1",
    "@hono/zod-validator": "^0.2.1",
    "@marplex/hono-azurefunc-adapter": "^1.0.1",
    "@orpc/client": "^1.4.5",
    "@orpc/server": "^1.4.5",
    "hono": "^4.2.4",
    "nodejs-polars": "^0.18.0",
