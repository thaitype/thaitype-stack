{
  "name": "pages-stack",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:web": "vite build",
    "build:func": "tsx beeze.ts build",
    "preview:func": "cd functions && func start",
    "dev": "run-p -l 'dev:*'",
    "dev:vite": "vite",
